{
	"env": {
		"es6": true,
		"node": true,
		"browser": true,
		"jasmine": true,
		"atomtest": true
	},
	"parserOptions": {
		"sourceType": "module",
		"ecmaVersion": 8,
		"ecmaFeatures": {
			"impliedStrict": true
		}
	},
	"globals": {
		"atom": false
	},
	"extends": "eslint:recommended",
	"rules": {
		"quotes": 1,
		"semi": 2
	}
}
